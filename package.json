{
  "name": "@lando/devtool",
  "description": "A next generation development tool.",
  "version": "4.0.0-unstable.6",
  "author": "Mike Pirog @pirog",
  "license": "GPL-3.0",
  "repository": "lando/core-next",
  "bugs": "https://github.com/lando/core-next/issues/new/choose",
  "homepage": "https://github.com/lando/core-next",
  "keywords": [
    "devtool",
    "devtool-plugin",
    "core"
  ],
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "devtool": "./bin/devtool"
  },
  "scripts": {
    "docs:build": "VPL_MVB_VERSION=$(git describe --tags --always --abbrev=1 --match=\"v[0-9].*\") vitepress build docs",
    "docs:dev": "VPL_BASE_URL=http://localhost:5173 VPL_MVB_VERSION=$(git describe --tags --always --abbrev=1 --match=\"v[0-9].*\") vitepress dev docs",
    "docs:mvb": "npx mvb docs",
    "docs:preview": "vitepress preview docs",
    "format": "prettier --write .",
    "lint": "eslint . --ext .cjs --ext .js --ext .mjs --ext .ts",
    "pkg": "pkg --config package.json --targets node20 --options dns-result-order=ipv4first bin/lando",
    "test:leia": "leia \"examples/**/README.md\" -c 'Destroy tests' --stdin",
    "test": "npm run lint && npm run test:unit"
  },
  "oclif": {
    "bin": "devtool",
    "dirname": "devtool",
    "name": "@lando/devtool",
    "plugins": [],
    "hooks": {
      "pre-config": [
        "./hooks/test.js",
        "./hooks/test2.js"
      ],
      "post-config": [
        "./hooks/test2.js",
        "./hooks/test.cjs"
      ]
    },
    "topicSeparator": ":"
  },
  "pkg": {
    "outputPath": "dist",
    "scripts": [
      "bin/*.js",
      "config/**/*.js",
      "hooks/**/*.js",
      "lib/**/*.js",
      "utils/**/*.js"
    ],
    "assets": [
      "config.yml",
      "package.json",
      "config/test.yaml",
      "config/user.yaml"
    ]
  },
  "dependencies": {
    "@lando/argv": "^1.1.2",
    "@npmcli/arborist": "^9.0.1",
    "@oclif/core": "^4.2.10",
    "axios": "^1.8.3",
    "camelcase-keys": "^9.1.3",
    "copy-dir": "^0.4.0",
    "debug": "^4.4.0",
    "dockerode": "^4.0.4",
    "fs-extra": "^11.1.1",
    "get-package-type": "^0.1.0",
    "is-class": "^0.0.9",
    "is-interactive": "^2.0.0",
    "is-root": "^3.0.0",
    "jsonfile": "^6.1.0",
    "kebabcase-keys": "^1.0.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^3.3.9",
    "nconf": "^0.12.0",
    "node-cache": "^4.1.1",
    "pacote": "^21.0.0",
    "semver": "^7.3.2",
    "slugify": "^1.6.5",
    "string-argv": "0.1.1",
    "valid-path": "^2.1.0",
    "valid-url": "^1.0.9",
    "which": "^5.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@lando/leia": "^1.0.0-beta.4",
    "@lando/vitepress-theme-default-plus": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-exports": "^1.0.0-beta.5",
    "eslint-import-resolver-typescript": "^3.8.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "prettier": "^3.5.3",
    "vitepress": "^1.6.3"
  }
}
