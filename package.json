{
  "name": "@lando/core",
  "description": "The libraries that power all of Lando.",
  "version": "4.0.0-unstable.6",
  "author": "Mike Pirog @pirog",
  "license": "GPL-3.0",
  "repository": "lando/core-next",
  "bugs": "https://github.com/lando/core-next/issues/new/choose",
  "homepage": "https://github.com/lando/core-next",
  "keywords": [
    "lando",
    "lando-plugin",
    "core"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "lando": "./bin/lando"
  },
  "exports": {
    "./app": "./lib/app.js",
    "./bootstrap": "./lib/bootstrap.js",
    "./config": "./lib/config.js",
    "./debug": "./lib/debug.js",
    "./file-storage": "./components/file-storage.js",
    "./no-storage": "./components/no-storage.js",
    "./lando": "./lib/product.js",
    "./product": "./lib/product.js",
    "./plugin": "./lib/plugin.js",
    "./templator": "./lib/templator.js",
    "./components/*": "./components/*.js",
    "./lib/*": "./lib/*.js",
    "./utils/*": "./utils/*.js"
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "recipes/**/*.js",
      "services/**/*.js",
      "types/**/*.js"
    ],
    "exclude": [
      "test/**"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs:dev": "vitepress dev docs --debug",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . --ext .js --ext .mjs ",
    "pkg": "pkg --config package.json --targets node20 --options dns-result-order=ipv4first bin/lando",
    "test:unit": "nyc --reporter=html --reporter=text mocha --timeout 5000 test/**/*.spec.js",
    "test:leia": "leia \"examples/**/README.md\" -c 'Destroy tests' --stdin",
    "test": "npm run lint && npm run test:unit"
  },
  "oclif": {
    "bin": "lando",
    "dirname": "lando",
    "name": "@lando/cli-next",
    "plugins": [],
    "hooks": {
      "minstrap": [
        "./hooks/test.js"
      ]
    },
    "topicSeparator": " "
  },
  "pkg": {
    "outputPath": "dist",
    "scripts": [
      "bin/*.js",
      "config/**/*.js",
      "hooks/**/*.js",
      "lib/**/*.js",
      "node_modules/@lando/**/*.cjs",
      "node_modules/@lando/**/*.js",
      "utils/**/*.js"
    ],
    "assets": [
      "FATCORE",
      "config.yml",
      "package.json",
      "config/test.yaml",
      "config/user.yaml",
      "node_modules/@lando/**/fastcgi_params",
      "node_modules/@lando/**/*.cnf",
      "node_modules/@lando/**/*.conf",
      "node_modules/@lando/**/*.ini",
      "node_modules/@lando/**/*.inc",
      "node_modules/@lando/**/*.json",
      "node_modules/@lando/**/*.php",
      "node_modules/@lando/**/*.ps1",
      "node_modules/@lando/**/*.py",
      "node_modules/@lando/**/*.sh",
      "node_modules/@lando/**/*.toml",
      "node_modules/@lando/**/*.tpl",
      "node_modules/@lando/**/*.tmpl",
      "node_modules/@lando/**/*.vcl",
      "node_modules/@lando/**/*.xml",
      "node_modules/@lando/**/*.yaml",
      "node_modules/@lando/**/*.yml",
      "node_modules/figlet/fonts/Small Slant.flf",
      "node_modules/figlet/fonts/ANSI Shadow.flf"
    ]
  },
  "dependencies": {
    "@lando/argv": "^1.1.2",
    "@lando/leia": "^1.0.0-beta.4",
    "@npmcli/arborist": "^6.2.9",
    "axios": "0.28.0",
    "camelcase-keys": "^7",
    "copy-dir": "^0.4.0",
    "dockerode": "^2.4.2",
    "fs-extra": "^11.1.1",
    "is-class": "^0.0.9",
    "js-yaml": "^4.1.0",
    "jsonfile": "^2.4.0",
    "kebabcase-keys": "^1.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3",
    "nconf": "^0.12.0",
    "node-cache": "^4.1.1",
    "pacote": "^15.1.3",
    "semver": "^7.3.2",
    "slugify": "^1.6.5",
    "string-argv": "0.1.1",
    "valid-url": "^1.0.9",
    "vitepress": "^1.0.0-rc.42",
    "which": "^2.0.2",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@lando/leia": "^0.6.4",
    "@lando/vitepress-theme-default-plus": "^1.1.0-beta.16",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^8.0.3",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0"
  }
}
